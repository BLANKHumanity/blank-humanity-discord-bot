	"$defs": {
		"command": {
			"allOf": [
				{
					"properties": {
						"description": {
							"type": "string",
							"description": "A Description for this command",
							"minLength": 5,
							"maxLength": 100
						},
						"hidden": {
							"type": "boolean"
						},
						"roleRestricted": {
							"type": "boolean",
							"default": false
						},
						"allowedRoles": {
							"type": "array",
							"items": {
								"type": "number"
							}
						},
						"options": {
							"type": "object"
						}
					},
					"required": [
						"description"
					]
				}
			],
			"if": {
				"properties": {
					"roleRestricted": {
						"const": false
					}
				},
				"required": "roleRestricted"
			},
			"else": {
				"required": [
					"allowedRoles"
				]
			}
		},
		"argument": {
			"type": "string",
			"description": "Description for Argument",
			"minLength": 3,
			"maxLength": 100
		},
		"message": {
			"type": "string",
			"pattern": "^([^\\%]|\\%\\([a-zA-Z]+\\)|\\%(?!\\w|\\())*$"
		},
		"item": {
			"type": "object",
			"properties": {
				"id": {
					"type": "number"
				},
				"name": {
					"type": "string"
				},
				"description": {
					"type": "string"
				},
				"useName": {
					"type": "string",
					"pattern": "^[a-zA-Z0-9]+$"
				},
				"actions": {
					"type": "array",
					"items": {
						"type": "object",
						"$ref": "#/$defs/itemAction"
					},
					"anyOf": [
						{
							"contains": {
								"properties": {
									"action": {
										"const": "MESSAGE_REPLY"
									}
								}
							}
						},
						{
							"contains": {
								"properties": {
									"action": {
										"const": "ROLE_REWARD"
									}
								}
							}
						}
					]
				}
			},
			"required": [
				"id",
				"name",
				"description",
				"useName"
			],
			"additionalProperties": false
		},
		"itemAction": {
			"type": "object",
			"properties": {
				"action": {
					"type": "string",
					"enum": [
						"ROLE_REWARD",
						"MESSAGE_SEND",
						"MESSAGE_REPLY",
						"REQUIRED_AMOUNT",
						"RANDOM_NUMBER",
						"RANDOM_SELECTION",
						"ITEM_REWARD"
					]
				},
				"selectors": {
					"type": "object"
				},
				"actionArguments": {
					"type": "object"
				}
			},
			"allOf": [
				{
					"if": {
						"properties": {
							"action": {
								"const": "ROLE_REWARD"
							}
						}
					},
					"then": {
						"$ref": "#/$defs/itemConfigKeys/roleId"
					}
				},
				{
					"if": {
						"properties": {
							"action": {
								"const": "MESSAGE_SEND"
							}
						}
					},
					"then": {
						"allOf": [
							{
								"$ref": "#/$defs/itemConfigKeys/message"
							},
							{
								"$ref": "#/$defs/itemConfigKeys/channel"
							}
						]
					}
				},
				{
					"if": {
						"properties": {
							"action": {
								"const": "MESSAGE_REPLY"
							}
						}
					},
					"then": {
						"$ref": "#/$defs/itemConfigKeys/message"
					}
				},
				{
					"if": {
						"properties": {
							"action": {
								"const": "REQUIRED_AMOUNT"
							}
						}
					},
					"then": {
						"allOf": [
							{
								"$ref": "#/$defs/itemConfigKeys/amount"
							},
							{
								"$ref": "#/$defs/itemConfigKeys/supportMultiUse"
							}
						]
					}
				},
				{
					"if": {
						"properties": {
							"action": {
								"const": "RANDOM_NUMBER"
							}
						}
					},
					"then": {
						"allOf": [
							{
								"$ref": "#/$defs/itemConfigKeys/name"
							},
							{
								"$ref": "#/$defs/itemConfigKeys/minimum"
							},
							{
								"$ref": "#/$defs/itemConfigKeys/maximum"
							}
						]
					}
				},
				{
					"if": {
						"properties": {
							"action": {
								"const": "RANDOM_SELECTION"
							}
						}
					},
					"then": {
						"allOf": [
							{
								"$ref": "#/$defs/itemConfigKeys/name"
							},
							{
								"properties": {
									"actionArguments": {
										"properties": {
											"selections": {
												"patternProperties": {
													"": {
														"type": "object",
														"properties": {
															"value": {
																"type": "string"
															},
															"probability": {
																"type": "number"
															}
														},
														"required": [
															"value",
															"probability"
														],
														"additionalProperties": false
													}
												},
												"minProperties": 2
											}
										},
										"required": [
											"selections"
										]
									}
								},
								"required": [
									"actionArguments"
								]
							}
						]
					}
				},
				{
					"if": {
						"properties": {
							"action": {
								"const": "ITEM_REWARD"
							}
						}
					},
					"then": {
						"allOf": [
							{
								"anyOf": [
									{
										"$ref": "#/$defs/itemConfigKeys/itemName"
									},
									{
										"$ref": "#/$defs/itemConfigKeys/itemId"
									}
								]
							},
							{
								"$ref": "#/$defs/itemConfigKeys/amount"
							}
						]
					}
				}
			]
		},
